#include "Eigenpair.h"

void Eigenpair::copy(const Eigenpair &orig){
    r   = orig.r;
    i   = orig.i;

    vrr = orig.vrr;
    vri = orig.vri;
    vlr = orig.vlr;
    vli = orig.vli;

    return;
}

Eigenpair::Eigenpair(){
}

Eigenpair::Eigenpair(int n){
    vrr.resize(n);
    vri.resize(n);
    vlr.resize(n);
    vlr.resize(n);
}

Eigenpair::Eigenpair(const Eigenpair &orig){
    copy(orig);
}

Eigenpair::Eigenpair(const Eigenpair *orig){
    copy(*orig);
}

Eigenpair::~Eigenpair(){
}


Eigenpair Eigenpair::operator=(const Eigenpair &orig){
    if (&orig != this) copy(orig);

    return *this;
}

