#include "CoreyQuad_Explicit_Bifurcation_Curves.h"

CoreyQuad_Explicit_Bifurcation_Curves::CoreyQuad_Explicit_Bifurcation_Curves(CoreyQuad *ff) : Three_Phase_Flow_Explicit_Bifurcation_Curves(ff){
}

CoreyQuad_Explicit_Bifurcation_Curves::~CoreyQuad_Explicit_Bifurcation_Curves(){
}

int CoreyQuad_Explicit_Bifurcation_Curves::check_flux_parameters(int &model_specific_error_code)
    RealVector flux_params = f->fluxParams().params();

    double grw = flux_params(0);
    double grg = flux_params(1);
    double gro = flux_params(2);

    // Only compute the curves in this case.
    //
    if (!(grw == grg && grw == gro)){
        return FLUX_PARAMETERS_ERROR;
    }
    else return FLUX_PARAMETERS_OK;
}

